"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n<!--      <el-input style=\"width: 200px\" placeholder=\"请输入\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"email\"></el-input>-->\r\n<!--      <el-input style=\"width: 200px\" placeholder=\"请输入\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>-->\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n      <!-- <el-upload action=\"http://localhost:9090/donate/import\" :show-file-list=\"false\" accept=\"xlsx\" :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\r\n        <el-button type=\"primary\" class=\"ml-5\">导入 <i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n      <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\">导出 <i class=\"el-icon-top\"></i></el-button> -->\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"  @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\" sortable></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"捐款人\"></el-table-column>\r\n      <el-table-column prop=\"goods\" label=\"捐赠物资\"></el-table-column>\r\n      <el-table-column prop=\"time\" label=\"捐款事件\"></el-table-column>\r\n      <el-table-column prop=\"expressNo\" label=\"快递单号\" width=\"180\"></el-table-column>\r\n      <el-table-column label=\"操作\"  width=\"180\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"100px\" size=\"small\" style=\"width: 90%\">\r\n        <el-form-item label=\"捐款人\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"捐赠物资\">\r\n          <el-input v-model=\"form.goods\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"捐款事件\">\r\n          <el-date-picker v-model=\"form.time\" type=\"datetime\" value-format=\"yyyy-MM-dd HH:mm:ss\" placeholder=\"选择日期时间\"></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"快递单号\">\r\n          <el-input v-model=\"form.expressNo\" placeholder=\"请输入快递单号\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <!-- 物流追踪弹窗 -->\r\n    <el-dialog title=\"物流追踪\" :visible.sync=\"expressDialogVisible\" width=\"40%\">\r\n      <div v-if=\"loadingExpress\" style=\"text-align:center;padding:20px;\">\r\n        <el-spinner></el-spinner>\r\n        <div>查询中...</div>\r\n      </div>\r\n      <div v-else>\r\n        <div v-if=\"expressTraces.length\">\r\n          <el-timeline>\r\n            <el-timeline-item\r\n              v-for=\"(item, idx) in expressTraces\"\r\n              :key=\"idx\"\r\n              :timestamp=\"item.time\"\r\n            >\r\n              {{ item.status }}\r\n            </el-timeline-item>\r\n          </el-timeline>\r\n        </div>\r\n        <div v-else style=\"color:#999;text-align:center;\">暂无物流信息</div>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"expressDialogVisible = false\">关闭</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: \"Donate\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      expressDialogVisible: false,\r\n      expressTraces: [],\r\n      loadingExpress: false,\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.request.get(\"/donate/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n      this.request.post(\"/donate\", this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/donate/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/donate/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(\"http://localhost:9090/donate/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    },\r\n    async trackExpress(row) {\r\n      this.expressDialogVisible = true;\r\n      this.loadingExpress = true;\r\n      this.expressTraces = [];\r\n      try {\r\n        // 快递100开放平台API\r\n        const apiKey = 'fPyfEVuh8409';\r\n        const com = row.expressCom; // 快递公司编码\r\n        const num = row.expressNo;  // 快递单号\r\n        // 官方文档：https://www.kuaidi100.com/openapi/api_post.shtml\r\n        const url = `https://www.kuaidi100.com/api/v1/track`;\r\n        const params = {\r\n          apiKey,\r\n          courier: com,\r\n          tracking_number: num,\r\n        };\r\n        // 兼容快递100新版API（如有变动请查阅官方文档）\r\n        const res = await axios.get(url, { params });\r\n        if (res.data && res.data.data && Array.isArray(res.data.data)) {\r\n          // 轨迹倒序，最新在前\r\n          this.expressTraces = res.data.data.map(item => ({\r\n            time: item.time || item.ftime,\r\n            status: item.context || item.status,\r\n          }));\r\n        } else {\r\n          this.expressTraces = [];\r\n        }\r\n      } catch (e) {\r\n        this.$message.error('物流查询失败');\r\n        this.expressTraces = [];\r\n      }\r\n      this.loadingExpress = false;\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>\r\n"],"mappings":";;AAkHA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAL,IAAA;MACAM,IAAA;MACAC,iBAAA;MACAC,iBAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,oBAAA;MACAC,aAAA;MACAC,cAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAlB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAL,IAAA,OAAAA;QACA;MACA,GAAAuB,IAAA,CAAAC,GAAA;QACA,KAAAtB,SAAA,GAAAsB,GAAA,CAAAvB,IAAA,CAAAwB,OAAA;QACA,KAAAtB,KAAA,GAAAqB,GAAA,CAAAvB,IAAA,CAAAE,KAAA;MACA;IACA;IACAuB,KAAA;MACA,KAAAN,OAAA,CAAAO,IAAA,iBAAArB,IAAA,EAAAiB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAI,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAvB,iBAAA;UACA,KAAAW,IAAA;QACA;UACA,KAAAW,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAC,UAAA;MACA,KAAAzB,iBAAA;MACA,KAAAD,IAAA;MACA,KAAA2B,SAAA;QACA,SAAAC,KAAA,CAAAC,GAAA;UACA,KAAAD,KAAA,CAAAC,GAAA,CAAAC,UAAA;QACA;QACA,SAAAF,KAAA,CAAAG,IAAA;UACA,KAAAH,KAAA,CAAAG,IAAA,CAAAD,UAAA;QACA;MACA;IACA;IACAE,WAAAC,GAAA;MACA,KAAAjC,IAAA,GAAAM,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAA4B,SAAA,CAAAD,GAAA;MACA,KAAAhC,iBAAA;MACA,KAAA0B,SAAA;QACA,SAAAC,KAAA,CAAAC,GAAA;UACA,KAAAD,KAAA,CAAAC,GAAA,CAAAC,UAAA;QACA;QACA,SAAAF,KAAA,CAAAG,IAAA;UACA,KAAAH,KAAA,CAAAG,IAAA,CAAAD,UAAA;QACA;MACA;IACA;IACAK,IAAAC,EAAA;MACA,KAAAtB,OAAA,CAAAuB,MAAA,cAAAD,EAAA,EAAAnB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAI,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAZ,IAAA;QACA;UACA,KAAAW,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAa,sBAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,KAAArC,iBAAA,GAAAqC,GAAA;I